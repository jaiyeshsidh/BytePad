{
  "name": "bytepad",
  "description": "A web application that provides previous years question papers to students.",
  "author": "Satvik Daga dagasatvik@gmail.com",
  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e src/router.js -o dist/build.js",
    "serve": "http-server -c 1 -a localhost",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify src/router.js | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "dependencies": {
    "underscore": "^1.8.3",
    "vue": "^1.0.0",
    "vue-resource": "^1.3.4",
    "vue-router": "^0.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.25.0",
    "babelify": "^7.3.0",
    "browserify": "^12.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.8",
    "http-server": "^0.9.0",
    "npm-run-all": "^1.6.0",
    "uglify-js": "^2.8.29",
    "uglifyjs": "^2.4.11",
    "vueify": "^8.7.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}
